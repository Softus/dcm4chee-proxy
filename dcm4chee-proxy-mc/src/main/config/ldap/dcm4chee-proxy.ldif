  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
  DESC 'Boolean : accept Associations'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
  DESC 'Boolean : initiate associations'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
  DESC 'Integer : max operations performed'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
  DESC 'Integer : max operations invoked'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.2.15.0.3.6 NAME 'dcmSpoolDirectory'
  DESC 'String : directory for temporary DICOM files'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.2.15.0.3.7 NAME 'dcmAcceptDataOnFailedNegotiation'
  DESC 'Boolean : temporary store data on failed negotiation with Called AET'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.2.15.0.3.8 NAME 'dcmDestinationAETitle'
  DESC 'String : send DICOM files to target Called AET'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.2.15.0.3.9 NAME 'dcmUseCallingAETitle'
  DESC 'String : value for Calling AE Title to be used by proxy'
  EQUALITY caseIgnoreMatch
