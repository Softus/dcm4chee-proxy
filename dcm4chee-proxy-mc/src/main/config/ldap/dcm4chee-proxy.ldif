  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.2.15.0.3.11 NAME 'dcmEnableAuditLog'
  DESC 'Boolean : enable audit logging'
