  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.2.15.0.3.10 NAME 'dcmExclusiveUseDefinedTC'
  DESC 'Boolean : only use transfer capabilities defined for proxy'
  EQUALITY caseIgnoreMatch
