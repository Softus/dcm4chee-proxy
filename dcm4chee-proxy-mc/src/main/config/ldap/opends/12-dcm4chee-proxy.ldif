# Extended DICOM Application Configuration Data Model Hierarchy LDAP Schema
dn: cn=dcm4chee-proxy,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: dcm4chee-proxy
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.1 NAME 'dcmSchedulerInterval'
  DESC 'Integer : interval to check for scheduled c-stores and logging'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.6 NAME 'dcmSpoolDirectory'
  DESC 'String : directory for temporary DICOM files'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.7 NAME 'dcmAcceptDataOnFailedNegotiation'
  DESC 'Boolean : temporary store data on failed negotiation with Called AET'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.8 NAME 'dcmDestinationAETitle'
  DESC 'String : send DICOM files to target Called AET'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.9 NAME 'dcmUseCallingAETitle'
  DESC 'String : value for Calling AE Title to be used by proxy'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.10 NAME 'dcmExclusiveUseDefinedTC'
  DESC 'Boolean : only use transfer capabilities defined for proxy'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.11 NAME 'dcmEnableAuditLog'
  DESC 'Boolean : enable audit logging'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.12 NAME 'dcmAuditDirectory'
  DESC 'String : audit log directory'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.13 NAME 'dcmScheduleDays'
  DESC 'Comma separated list or range : days for availability of AE'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.14 NAME 'dcmScheduleHours'
  DESC 'Comma separated list or range : hours for availability of AE'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.15 NAME 'dcmRetrySuffix'
  DESC 'Suffix of retry case'
  EQUALITY caseIgnoreMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.16 NAME 'dcmRetryDelay'
  DESC 'Delay before trying to resend'
  EQUALITY caseIgnoreMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.17 NAME 'dcmRetryNum'
  DESC 'Number of failed retries'
  EQUALITY caseIgnoreMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.18 NAME 'dcmNactionDirectory'
  DESC 'String : naction spool directory'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.19 NAME 'dcmNeventDirectory'
  DESC 'String : nevent spool directory'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.20 NAME 'dcmDefaultDestinationAETitle'
  DESC 'String : fallback AET for sending DICOM files to'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
attributeTypes: ( 1.2.40.0.13.1.2.15.0.3.21 NAME 'dcmCallingAETitle'
  DESC 'String : value for Calling AE Title specific Forward Rule'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
objectClasses: ( 1.2.40.0.13.1.2.15.0.4.1 NAME 'dcmProxyDevice'
  DESC 'DICOM Proxy Device related information'
  SUP top AUXILIARY
  MUST (
    dcmSchedulerInterval ) )
objectClasses: ( 1.2.40.0.13.1.2.15.0.4.2 NAME 'dcmProxyNetworkAE'
  DESC 'DICOM Proxy Network AE related information'
  SUP top AUXILIARY
  MUST (
    dcmSpoolDirectory $
    dcmAcceptDataOnFailedNegotiation $
    dcmEnableAuditLog $
    dcmNactionDirectory $
    dcmNeventDirectory $
    dcmDefaultDestinationAETitle )
  MAY (
    dcmAuditDirectory) )
objectClasses: ( 1.2.40.0.13.1.2.15.0.4.3 NAME 'dcmRetry'
  DESC 'Retry configuration for specific cases'
  SUP top STRUCTURAL
  MUST (
    dcmRetrySuffix $
    dcmRetryDelay $
    dcmRetryNum ) )
objectClasses: ( 1.2.40.0.13.1.2.15.0.4.4 NAME 'dcmForwardSchedule'
  DESC 'Forward Schedule configuration'
  SUP top
  STRUCTURAL
  MUST (
    dcmScheduleDays $
    dcmScheduleHours $
    dcmDestinationAETitle ) )
objectClasses: ( 1.2.40.0.13.1.2.15.0.4.5 NAME 'dcmForwardRule'
  DESC 'Forward Rule configuration'
  SUP top
  STRUCTURAL
  MUST (
    labeledURI $
    commonName )
  MAY (
    dcmDIMSE $
    dicomSOPClass $
    dcmCallingAETitle $
    dcmExclusiveUseDefinedTC $
    dcmUseCallingAETitle $
    dcmScheduleDays $
    dcmScheduleHours) )

 